        <th>
            @Html.DisplayNameFor(model => model.tbBodega1.bod_Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbEstadoMovimiento.estm_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbFactura.fact_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbTipoDevolucion.tdev_Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbTipoSalida.tsal_Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sal_FechaElaboracion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sal_EsAnulada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sal_RazonAnulada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sal_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sal_FechaModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuario.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbUsuario1.usu_NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbBodega.bod_Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbBodega1.bod_Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbEstadoMovimiento.estm_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbFactura.fact_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbTipoDevolucion.tdev_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbTipoSalida.tsal_Descripcion)
        </td>
        <td>
			@Html.ActionLink("Details", "Details", new { id=item.sal_Id }, new { @class = "btn btn-primary btn-sm"})
